{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Update contact schema",
  "required": [
    "name",
    "phone"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "The name schema",
      "minLength": 3
    },
    "phone": {
      "type": "integer",
      "title": "The phone schema",
      "minLength": 10,
      "maxLength": 10
    }
  },
  "additionalProperties": false
}
