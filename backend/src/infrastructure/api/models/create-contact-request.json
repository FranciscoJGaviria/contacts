{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Create contact schema",
  "required": [
    "contactId",
    "name",
    "phone"
  ],
  "properties": {
    "contactId": {
      "type": "string",
      "title": "The contactId schema",
      "minLength": 36,
      "maxLength": 36
    },
    "name": {
      "type": "string",
      "title": "The name schema",
      "minLength": 3

    },
    "phone": {
      "type": "string",
      "title": "The phone schema",
      "minLength": 10,
      "maxLength": 10
    }
  },
  "additionalProperties": false
}
